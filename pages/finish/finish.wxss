/* pages/finish/finish.wxss */
.header {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #FFC125;
  font-size: 40rpx;
  color: white;
  text-align: center;
  position: fixed;
  top: 0;
  z-index: 1000;
}
.outer-wrap {
  width: 100%;
  position: absolute;
  top: 80rpx;
  padding-bottom: 120rpx;
}
.service-no-data {
  text-align: center;
  font-size: 32rpx;
  color: #BEBEBE;
  line-height: 800rpx;
}
.service-list {
  height: auto;
  /* margin: 4rpx auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between; */
  border: 1px solid #f2f2f2;
  font-size: 28rpx;
  padding: 0 20rpx;
}
.service-list .service-item {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  line-height: 48rpx;
  
}
.service-list .service-item .item-first {
  padding-right: 50rpx;
}
.service-list .service-item image {
  width: 48rpx;
  height: 48rpx;
  vertical-align: bottom;
}
.service-list .service-item.first {
  line-height: 60rpx;
  font-size: 32rpx;
  font-weight: bold;
}
.footer {
  width: 100%;
  position: fixed;
  bottom: 0;
  text-align: center;
  background: #fff;
  padding-top: 20rpx; 
  line-height: 80rpx;
  border-top: 1px solid #f2f2f2;
}
.footer text {
  width: 100%;
  font-size: 24rpx;
  color: #515151;
}
.footer .btn {
  width: 280rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20rpx;
  font-size: 28rpx;
}